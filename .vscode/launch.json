    "version": "0.2.0",
    "configurations": [
    

        {
            "name": "Kedro Run",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run"]
            // Any other arguments should be passed as a comma-seperated-list
            // e.g "args": ["run", "--pipeline", "pipeline_name"]
        },
        {
            "name": "Run Prepare Data",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run",  "--pipeline", "prep", "--env", "env_opt"]
        },
        {
            "name": "Run IRBS",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run",  "--pipeline", "irbs", "--env", "env_opt"]
        },
        {
            "name": "Run Analysis",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run",  "--pipeline", "analysis", "--env", "env_opt"]
        },
        {
            "name": "Run SAO optim",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run",  "--pipeline", "sao_optim", "--env", "env_opt"]
        },
        {
            "name": "Run ParEGO",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run",  "--pipeline", "parego", "--env", "env_opt"]
        },
        {
            "name": "Run metrics",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": ["run",  "--pipeline", "metrics", "--env", "env_opt"]
        }
    ]
